<div class="index">
  <% @describes.each do |describe| %>
   <div class="contribution">
     <%= link_to describe, class: "link"  do%>
       <p>
         <%= image_tag describe.user.profile_image.url %>
         <%= describe.user.name %>
         <i class="fas fa-heart" style="color:red"></i> <%= describe.likes_count %>
       </p>
       <p>
         <%= describe.title %>
       </p>
     <% end %>
  </div>
  <% end %>
  <div class="pages">
    <p class="page"><%= paginate @describes %></p>
  </div>
</div>
